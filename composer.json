{
    "name": "geniuslinchao/laravel-PSRedis",
    "description": "A simple sentinel/redis driver wrapper for laravel.",
    "authors": [
        {
            "name": "Damien Russell(origin)/geniuslinchao",
            "email": "drussell@indatus.com(origin)/geniuslinchao@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "illuminate/support": ">=5.0",
        "geniuslinchao/psredis": ">=1.2.1"
    },
    "repositories": [
        {       
                "type": "vcs",
                "url": "https://github.com/geniuslinchao/PSRedis"
        }
     ],
    "require-dev": {
        "laravel/framework": ">=5.0",
        "phpunit/phpunit": ">=4.0",
        "mockery/mockery": ">=0.9"
    },
    "autoload": {
        "classmap": [
            "src/lib"
        ],
        "psr-0": {
            "Indatus\\LaravelPSRedis\\": "src/"
        }
    },
    "psr-4": {
        "": "src/lib"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
